{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "fiip",
  "version": "0.9.8",
  "identifier": "com.darkii.fiip",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "fiip",
        "width": 800,
        "height": 600,
        "decorations": false,
        "transparent": true,
        "shadow": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "plugins": {
    "updater": {
      "endpoints": [
        "https://github.com/darkiifr/Fiip/releases/latest/download/latest.json"
      ],
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDk3QjA3RUM2NTQ4NDRBMkMKUldRc1NvUlV4bjZ3bDQzSWJ2bHFCMmd5Q0FCUUxBVStjaGNFZVEycldDdEZDMzYzZTdvb29yYXMK"
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}